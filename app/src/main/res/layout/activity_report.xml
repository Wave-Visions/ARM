<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fbutton="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:padding="10dp"
    android:background="@color/button_border"
    android:id="@+id/main_lay"
    tools:context="aim.aiminmed.server.aimarm.report">

    <LinearLayout
        android:id="@+id/report"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_weight="1"
        android:background="@color/White">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/blue_light"
        android:elevation="10dp"
        android:layout_weight="1">


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:gravity="center_horizontal"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_weight="0.5"
                android:background="@drawable/medtech_dev" />

        </LinearLayout>



        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="5dp"
            android:layout_weight="1.5">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Date"
            android:layout_weight="1"
            android:textColor="@color/Black"
            android:id="@+id/date" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Time"
                android:layout_weight="1"
                android:textColor="@color/Black"
                android:id="@+id/time" />

        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="3dp"
        android:background="@color/Light_coffee"
        android:layout_weight="0.96">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Doctor Name"
            android:paddingStart="10dp"
            android:background="@color/Lighter_blue"
            android:id="@+id/textView23"
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text=":"
            android:textSize="20sp"
            android:layout_weight="1.8"
            android:background="@color/Lighter_blue"
            android:id="@+id/textView46" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Name"
            android:background="@color/Lighter_blue"
            android:id="@+id/doc_name"
            android:textSize="20sp"
            android:layout_weight="1"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="3dp"
        android:background="@color/Light_coffee"
        android:layout_weight="0.96">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Patient Name"
            android:id="@+id/textView26"
            android:paddingStart="10dp"
            android:background="@color/Lighter_blue"
            android:textSize="20sp"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text=":"
            android:textSize="20sp"
            android:layout_weight="1.8"
            android:background="@color/Lighter_blue"
            android:id="@+id/textView47" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Name"
            android:id="@+id/pat_name"
            android:textSize="20sp"
            android:background="@color/Lighter_blue"
            android:layout_weight="1"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="5dp"
        android:background="@color/Light_coffee"
        android:layout_weight="1">


        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Age"
            android:id="@+id/textView28"
            android:textSize="20sp"
            android:paddingStart="10dp"
            android:background="@color/Lighter_blue"
            android:layout_weight="1"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text=":"
            android:textSize="20sp"
            android:layout_weight="1.8"
            android:background="@color/Lighter_blue"
            android:id="@+id/textView45" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Number"
            android:id="@+id/age"
            android:background="@color/Lighter_blue"
            android:textSize="20sp"
            android:layout_weight="1"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="0.5">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:elevation="10dp"
            android:layout_weight="1">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Anorectal Manometry Results"
                android:textSize="20sp"
                android:gravity="center"
                android:background="@color/blue_light"
                android:textColor="@color/White"
                android:id="@+id/textView30" />
        </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="5dp"
            android:background="@color/Light_coffee"
            android:layout_weight="0.9">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Modes"
                android:id="@+id/textView32"
                android:textSize="20sp"
                android:paddingStart="20dp"
                android:textColor="@color/blue"
                android:layout_weight="1"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Average (mmhg)"
                android:id="@+id/textView33"
                android:textSize="20sp"
                android:paddingStart="15dp"
                android:textColor="@color/blue"
                android:layout_weight="1"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="20sp"
                android:paddingLeft="20dp"
                android:text="Max (mmhg)"
                android:textColor="@color/blue"
                android:layout_weight="1"
                android:id="@+id/textView25" />


        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="5dp"
            android:background="@color/Lighter_blue"
            android:layout_weight="1">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Rest"
                android:layout_weight="1"
                android:paddingStart="20dp"
                android:textSize="20sp"
                android:background="@color/Light_coffee"
                android:id="@+id/textView34" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="0"
                android:paddingStart="45dp"
                android:layout_weight="1"
                android:textSize="20sp"
                android:background="@color/Light_coffee"
                android:id="@+id/r_avg" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="0"
                android:paddingStart="45dp"
                android:textSize="20sp"
                android:layout_weight="1"
                android:background="@color/Light_coffee"
                android:id="@+id/r_max" />
        </LinearLayout>



    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="5dp"
        android:background="@color/Lighter_blue"
        android:layout_weight="1">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Squeeze"
            android:layout_weight="1"
            android:textSize="20sp"
            android:paddingStart="20dp"
            android:background="@color/Light_coffee"
            android:id="@+id/textView36" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="0"
            android:layout_weight="1"
            android:textSize="20sp"
            android:paddingStart="45dp"
            android:background="@color/Light_coffee"
            android:id="@+id/s_avg" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="0"
            android:paddingStart="45dp"
            android:textSize="20sp"
            android:layout_weight="1"
            android:background="@color/Light_coffee"
            android:id="@+id/s_max" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="5dp"
        android:background="@color/Lighter_blue"
        android:layout_weight="1">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Cough"
            android:layout_weight="1"
            android:textSize="20sp"
            android:paddingStart="20dp"
            android:background="@color/Light_coffee"
            android:id="@+id/textView38" />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="0"
            android:layout_weight="1"
            android:textSize="20sp"
            android:paddingLeft="45dp"
            android:background="@color/Light_coffee"
            android:id="@+id/c_avg" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="0"
            android:layout_weight="1"
            android:textSize="20sp"
            android:paddingStart="45dp"
            android:background="@color/Light_coffee"
            android:id="@+id/c_max" />
    </LinearLayout>

    </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="9">



        <info.hoang8f.widget.FButton
            android:id="@+id/share_button"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="left|top"
            android:layout_marginTop="5dp"
            android:minHeight="@dimen/demo_button_min_height"
            android:minWidth="@dimen/demo_button_min_width"
            android:text="Submit"
            android:textColor="@android:color/white"
            fbutton:buttonColor="@color/fbutton_color_peter_river"
            fbutton:shadowEnabled="true" />

        </LinearLayout>





    </LinearLayout>
