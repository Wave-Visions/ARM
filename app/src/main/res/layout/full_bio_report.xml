
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="aim.aiminmed.server.aimarm.pic_report"
    android:id="@+id/main"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:layout_weight="1"
        android:id="@+id/scrollView3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/White">

    <LinearLayout
        android:id="@+id/report"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/White">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:elevation="10dp"
            android:layout_weight="1">


            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/aim"
                android:padding="5dp"
                android:layout_weight="3"
                android:id="@+id/imageButton2" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Aim In Med"
                android:textSize="15sp"
                android:layout_weight="2.8"
                android:gravity="center"
                android:textColor="@color/Black"
                android:id="@+id/textView" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_weight="1">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Bio feedback Report"
                    android:textSize="20sp"
                    android:gravity="center"
                    android:textColor="@color/Black"
                    android:layout_weight="1"
                    android:id="@+id/rp" />


                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    android:layout_weight="1">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Date"
                        android:gravity="center"
                        android:layout_weight="1"

                        android:textColor="@color/Black"
                        android:id="@+id/date" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Time"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:textColor="@color/Black"
                        android:id="@+id/time" />

                </LinearLayout>

            </LinearLayout>



        </LinearLayout>
        </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="3dp"
                android:background="@color/Light_coffee"
                android:layout_weight="0.96">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Hospital Name"
                    android:paddingStart="10dp"
                    android:textStyle="bold"
                    android:background="@color/Lighter_blue"
                    android:id="@+id/textView73"
                    android:textSize="15sp"
                    android:layout_weight="1"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=":"
                    android:textSize="15sp"
                    android:layout_weight="1.8"
                    android:background="@color/Lighter_blue"
                    android:id="@+id/textView56" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Name"
                    android:textStyle="bold"
                    android:background="@color/Lighter_blue"
                    android:id="@+id/hos"
                    android:textSize="15sp"
                    android:layout_weight="1"/>
            </LinearLayout>


        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="3dp"
            android:background="@color/Light_coffee"
            android:layout_weight="0.96">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Doctor Name"
                android:paddingStart="10dp"
                android:textStyle="bold"
                android:background="@color/Lighter_blue"
                android:id="@+id/textView23"
                android:textSize="15sp"
                android:layout_weight="1"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=":"
                android:textSize="15sp"
                android:layout_weight="1.8"
                android:background="@color/Lighter_blue"
                android:id="@+id/textView46" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Name"
                android:textStyle="bold"
                android:background="@color/Lighter_blue"
                android:id="@+id/doc"
                android:textSize="15sp"
                android:layout_weight="1"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="3dp"
            android:background="@color/Light_coffee"
            android:layout_weight="0.96">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Patient Name"
                android:id="@+id/textView26"
                android:paddingStart="10dp"
                android:background="@color/Lighter_blue"
                android:textSize="15sp"
                android:layout_weight="1"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=":"
                android:textSize="15sp"
                android:layout_weight="1.8"
                android:background="@color/Lighter_blue"
                android:id="@+id/textView47" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Name"
                android:id="@+id/pat"
                android:textSize="15sp"
                android:background="@color/Lighter_blue"
                android:layout_weight="1"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="3dp"
            android:background="@color/Light_coffee"
            android:layout_weight="1">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Age"
                android:id="@+id/textView28"
                android:textSize="15sp"
                android:paddingStart="10dp"
                android:background="@color/Lighter_blue"
                android:layout_weight="1"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=":"
                android:textSize="15sp"
                android:layout_weight="1.8"
                android:background="@color/Lighter_blue"
                android:id="@+id/textView45" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Number"
                android:id="@+id/ag"
                android:background="@color/Lighter_blue"
                android:textSize="15sp"
                android:layout_weight="1"/>
        </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="3dp"
                android:background="@color/Light_coffee"
                android:layout_weight="0.96">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Remarks"
                    android:paddingStart="10dp"
                    android:background="@color/Lighter_blue"
                    android:id="@+id/textView83"
                    android:textSize="15sp"
                    android:layout_weight="1"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text=":"
                    android:textSize="15sp"
                    android:layout_weight="1.8"
                    android:background="@color/Lighter_blue"
                    android:id="@+id/textView66" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="None"
                    android:background="@color/Lighter_blue"
                    android:id="@+id/rm"
                    android:textSize="15sp"
                    android:layout_weight="1"/>
            </LinearLayout>





        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_weight="0.5"
            android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:background="@color/Lighter_blue">



            <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/air" />


        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:background="@color/White">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:id="@+id/rst" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Rest Mode"
                android:textSize="20sp"
                android:gravity="center"
                android:background="@color/Lighter_blue"
                android:id="@+id/textView37" />



            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Avg :"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:id="@+id/name3" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_weight="1.5"
                    android:id="@+id/r_t" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Max :"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:id="@+id/name4" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:layout_weight="1"
                    android:id="@+id/r_mt" />
            </LinearLayout>
            </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:background="@color/White">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Squeeze Mode"
                android:gravity="center"
                android:textSize="20sp"
                android:background="@color/Lighter_blue"
                android:id="@+id/textView39" />

            <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/sqz" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:gravity="left"
                    android:layout_weight="1"
                    android:id="@+id/s_t" />


            </LinearLayout>
            </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:background="@color/White">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Relax Mode"
                android:textSize="20sp"
                android:gravity="center"
                android:background="@color/Lighter_blue"
                android:id="@+id/textView40" />

            <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/rlx" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:gravity="left"

                    android:layout_weight="1"
                    android:id="@+id/rx_t" />

            </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:background="@color/White">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="Best Trials"
                    android:textColor="@color/Black"
                    android:textSize="20sp"
                    android:gravity="center"
                    android:background="@color/Lighter_blue"
                    android:id="@+id/textView400" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Best Mean Time"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:id="@+id/textView401" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:id="@+id/trial1" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Best Max Pressure"
                    android:textStyle="bold"
                    android:textSize="15sp"

                    android:id="@+id/textView402" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:id="@+id/trial2" />

            </LinearLayout>


        </LinearLayout>




</LinearLayout>
    </ScrollView>

    <Button
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Generate PDF"
        android:layout_gravity="center"
        android:layout_weight="10"
        android:id="@+id/print" />

</LinearLayout>
